 #000;\n  color: #fff;\n  border-radius: 5px;\n  position: relative;\n  display: flex;\n  z-index: 5;\n  border-radius: 5px;\n  opacity: 0;\n  pointer-events: none;\n  transition: all 0.4s ease-in-out;\n  transform: translateY(8px);\n}\n\n.modal.is-active {\n  opacity: 1;\n  pointer-events: all;\n  transform: translateY(-8px);\n}\n\n@media screen and (max-width: 1160px) {\n  .modal {\n    width: 100%;\n    height: 35vh;\n    max-width: 90%;\n    margin: auto;\n  }\n\n  .title {\n    font-size: 20px;\n  }\n\n  .btn {\n    font-size: 10px;\n    width: 50%;\n    margin: auto;\n  }\n}\n\n.left-content {\n  background: url(\"../images/car-20.jpg\") no-repeat center center / cover;\n}\n\n.right-content,\n.left-content {\n  width: 50%;\n  height: 100%;\n}\n\n.right-content {\n  height: 100%;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  text-align: center;\n}\n\n.right-content .content .form {\n  width: 100%;\n}\n\n.register-form {\n  display: flex;\n  flex-direction: column;\n}\n\n@media screen and (max-width: 860px) {\n  .input-field {\n    font-size: 10px;\n    width: 200px;\n  }\n}\n\n.close-btn {\n  cursor: pointer;\n  position: absolute;\n  top: 0;\n  right: 0;\n  z-index: 10;\n}\n"},"lineCount":null}},"error":null,"hash":"485ba3a2c03a2b8024b50a83e55408d4","cacheData":{"env":{}}}